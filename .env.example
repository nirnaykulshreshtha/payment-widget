# Copy this file to .env and fill in the values before running Hardhat scripts.
# Never commit your real keys.

# Private key used for contract deployment (0x-prefixed, without quotes).
FORWARDER_DEPLOYER_PRIVATE_KEY=0xabc123...

# Optional override for the CallForwarder owner; defaults to the deployer if unset.
FORWARDER_OWNER=0xownerAddress...

# Ethereum
MAINNET_RPC_URL=https://mainnet.infura.io/v3/your-key
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your-key

# Polygon
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/your-key
POLYGON_AMOY_RPC_URL=https://polygon-amoy.g.alchemy.com/v2/your-key

# BNB Chain
BNB_RPC_URL=https://bsc-dataseed.binance.org
BNB_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545

# Base
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Optimism
OPTIMISM_RPC_URL=https://mainnet.optimism.io
OPTIMISM_SEPOLIA_RPC_URL=https://sepolia.optimism.io

# Arbitrum
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
ARBITRUM_SEPOLIA_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc
